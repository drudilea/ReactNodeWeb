{
  "name": "react-node-project",
  "version": "1.0.0",
  "description": "React Node Project (MERN Stack)",
  "scripts": {
    "client": "cd ./front && npm start",
    "server": "cd ./back && nodemon index.js",
    "start-dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "inst-dev": "npm install && cd ./front && npm install",
    "start": "cd ./back && node index.js",
    "build": "cd ./front && npm install && npm run build"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "author": "Leandro Drudi",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.3.0",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.12",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6"
  }
}
